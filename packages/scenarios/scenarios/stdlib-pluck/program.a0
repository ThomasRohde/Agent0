let people = [
  { name: "Alice", age: 30 },
  { name: "Bob", age: 25 },
  { name: "Charlie", age: 35 }
]
let names = pluck { in: people, key: "name" }
let ages = pluck { in: people, key: "age" }
let missing = pluck { in: people, key: "email" }
let mixed = pluck { in: [{ x: 1 }, 42, null, { x: 3 }], key: "x" }
let empty = pluck { in: [], key: "any" }
return {
  names: names,
  ages: ages,
  missing: missing,
  mixed: mixed,
  empty: empty
}
